namespace net.biz.digitalPropertyNetwork

asset patientAsset identified by patientId {
  o String   patientId
  --> Person  patient
  o String   information
  o String  claimGranted 
}

asset claimContract identified by claimId {
  o String    claimId
  --> Person    insurer
  --> Person    patient
  o String 		startDate
  o String 		endDate
  o String 		claimAmount
}

participant Person identified by personId {
  o String personId
  o String firstName
  o String lastName
}


transaction processClaims {
  --> Person patient
  --> Person insurer

}
